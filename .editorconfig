root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[**.md]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false

[**.js]
indent_style = space
indent_size = 4

[**.json]
indent_style = space
indent_size = 4

[.*rc]
indent_style = space
indent_size = 4
